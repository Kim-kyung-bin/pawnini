<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias alias="paws" type="com.pawnini.model.paws.PawsDTO"/>
		<typeAlias alias="cs" type="com.pawnini.model.cs.CsDTO"/>
		<typeAlias alias="cs_reply" type="com.pawnini.model.cs.Cs_replyDTO"/>
		<typeAlias alias="adoption" type="com.pawnini.model.adoption.AdoptionDTO"/>
	</typeAliases>
	
	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/adoption-mapping.xml"/>
		<mapper resource="mappings/cs-mapping.xml"/>
		<mapper resource="mappings/cs_reply-mapping.xml"/>
		<mapper resource="mappings/paws-mapping.xml"/>		
	</mappers>
	
</configuration>
