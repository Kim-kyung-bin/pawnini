<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias alias="paws" type="com.pawnini.model.paws.PawsDTO"/>
		<typeAlias alias="adoption" type="com.pawnini.model.adoption.AdoptionDTO"/>
	</typeAliases>
	
	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/paws/paws-mapping.xml"/>
		<mapper resource="mappings/adoption/adoption-mapping.xml"/>
	</mappers>
</configuration>
